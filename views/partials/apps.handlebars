<div id="app-{{ id }}" class="app-card">
    <a href="{{ app_repo_URL }}">
        <div class="app-text">
            <h5>{{ app_name }}</h5>
            <p>
                {{ app_subtitle }}
            </p>
        </div>
        <img src="{{ app_img_URL }}" alt="{{ app_name }}">
    </a>
</div>
